{
  "name": "@takomo/organization-schema",
  "version": "2.10.1",
  "description": "Takomo Organization Schema",
  "author": "henri@takomo.io",
  "license": "MIT",
  "os": [
    "darwin",
    "linux"
  ],
  "engines": {
    "node": ">=14.4.0"
  },
  "homepage": "https://takomo.io",
  "bugs": "https://github.com/takomo-io/takomo/issues",
  "type": "commonjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/takomo-io/takomo.git"
  },
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean:dist": "rm -rf dist",
    "clean:deps": "rm -rf node_modules",
    "clean:all": "rm -rf node_modules dist && rm -f yarn.lock",
    "watch": "tsc -p tsconfig.build.json --watch",
    "depcheck": "depcheck --config=../../.depcheckrc",
    "test": "jest test --passWithNoTests"
  },
  "dependencies": {
    "@takomo/config-sets": "2.10.1",
    "@takomo/core": "2.10.0",
    "joi": "17.2.1"
  },
  "devDependencies": {
    "@takomo/unit-test": "2.9.0"
  }
}
