{
  "name": "@takomo/cli",
  "version": "1.2.1",
  "description": "Takomo CLI",
  "author": "henri@takomo.io",
  "license": "MIT",
  "homepage": "https://takomo.io",
  "bugs": "https://github.com/takomo-io/takomo/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/takomo-io/takomo.git"
  },
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean:dist": "rm -rf dist",
    "clean:deps": "rm -rf node_modules",
    "clean:all": "rm -rf node_modules dist && rm -f yarn.lock",
    "watch": "tsc -p tsconfig.build.json --watch",
    "depcheck": "depcheck --ignore-dirs=dist --ignores=@types/jest",
    "test": "jest test --passWithNoTests"
  },
  "dependencies": {
    "@takomo/cli-io": "1.2.1",
    "@takomo/config-sets": "1.2.1",
    "@takomo/core": "1.2.1",
    "@takomo/deployment-targets": "1.2.1",
    "@takomo/organization": "1.2.1",
    "@takomo/stacks-commands": "1.2.1",
    "@takomo/util": "1.2.1",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "easy-table": "1.1.1",
    "lodash.merge": "4.6.2",
    "pretty-ms": "5.1.0",
    "yargs": "13.3.0"
  },
  "devDependencies": {
    "@types/easy-table": "0.0.32",
    "@types/jest": "25.1.1",
    "@types/lodash.merge": "4.6.6",
    "@types/yargs": "13.0.3",
    "jest": "25.5.3",
    "ts-jest": "25.2.1",
    "typescript": "3.9.3"
  }
}
