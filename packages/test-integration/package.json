{
  "name": "@takomo/test-integration",
  "version": "3.0.0",
  "description": "Takomo integration test utils",
  "homepage": "https://takomo.io",
  "bugs": "https://github.com/takomo-io/takomo/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/takomo-io/takomo.git"
  },
  "license": "MIT",
  "author": "henri@takomo.io",
  "type": "commonjs",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build-if-changed": "../../ci/build-if-changed.sh",
    "clean:all": "rm -rf node_modules dist && rm -f yarn.lock",
    "clean:deps": "rm -rf node_modules",
    "clean:dist": "rm -rf dist",
    "depcheck": "depcheck --config=../../.depcheckrc",
    "test": "jest test --passWithNoTests --verbose",
    "watch": "tsc -p tsconfig.build.json --watch"
  },
  "dependencies": {
    "@takomo/aws-clients": "3.0.0",
    "@takomo/aws-model": "3.0.0",
    "@takomo/cli": "3.0.0",
    "@takomo/cli-io": "3.0.0",
    "@takomo/config-repository-fs": "3.0.0",
    "@takomo/core": "3.0.0",
    "@takomo/deployment-targets-commands": "^3.0.0",
    "@takomo/deployment-targets-context": "^3.0.0",
    "@takomo/init-command": "^3.0.0",
    "@takomo/organization-commands": "^3.0.0",
    "@takomo/organization-context": "^3.0.0",
    "@takomo/organization-model": "^3.0.0",
    "@takomo/stacks-commands": "^3.0.0",
    "@takomo/stacks-context": "^3.0.0",
    "@takomo/stacks-model": "^3.0.0",
    "@takomo/util": "^3.0.0",
    "aws-sdk": "2.745.0"
  },
  "engines": {
    "node": ">=14.4.0"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "publishConfig": {
    "access": "public"
  }
}
