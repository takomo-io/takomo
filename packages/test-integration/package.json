{
  "name": "@takomo/test-integration",
  "version": "4.0.0",
  "description": "Takomo integration test utils",
  "homepage": "https://takomo.io",
  "bugs": "https://github.com/takomo-io/takomo/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/takomo-io/takomo.git"
  },
  "license": "MIT",
  "author": "henri@takomo.io",
  "type": "commonjs",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build-if-changed": "../../ci/build-if-changed.sh",
    "clean:all": "rm -rf node_modules dist && rm -f yarn.lock && rm -rf .checksum",
    "clean:deps": "rm -rf node_modules",
    "clean:dist": "rm -rf dist",
    "depcheck": "depcheck --config=../../.depcheckrc",
    "test": "jest test --passWithNoTests --verbose",
    "watch": "tsc -p tsconfig.build.json --watch"
  },
  "dependencies": {
    "@aws-sdk/client-cloudformation": "3.53.0",
    "@aws-sdk/client-ec2": "3.53.0",
    "@aws-sdk/client-secrets-manager": "3.53.0",
    "@aws-sdk/client-ssm": "3.53.0",
    "@aws-sdk/client-sts": "3.53.0",
    "@aws-sdk/types": "3.53.0",
    "@takomo/aws-clients": "4.0.0",
    "@takomo/aws-model": "4.0.0",
    "@takomo/cli": "4.0.0",
    "@takomo/cli-io": "4.0.0",
    "@takomo/config-repository-fs": "4.0.0",
    "@takomo/config-sets": "4.0.0",
    "@takomo/core": "4.0.0",
    "@takomo/deployment-targets-commands": "4.0.0",
    "@takomo/deployment-targets-context": "4.0.0",
    "@takomo/deployment-targets-model": "4.0.0",
    "@takomo/execution-plans": "4.0.0",
    "@takomo/init-command": "4.0.0",
    "@takomo/stacks-commands": "4.0.0",
    "@takomo/stacks-context": "4.0.0",
    "@takomo/stacks-model": "4.0.0",
    "@takomo/test-unit": "4.0.0",
    "@takomo/util": "4.0.0"
  },
  "engines": {
    "node": ">=14.17.1"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "publishConfig": {
    "access": "public"
  }
}
