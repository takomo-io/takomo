    "@takomo/stacks-commands": "0.2.0",
    "@takomo/core": "0.2.0",
    "@takomo/util": "0.2.0",
    "lodash.flatten": "4.4.0",
    "lodash.intersection": "4.4.0",
    "lodash.last": "3.0.0",
    "lodash.merge": "4.6.2",
    "lodash.takerightwhile": "4.6.0",
    "lodash.uniq": "4.5.0",
    "lodash.uniqby": "4.7.0",
    "lodash.without": "4.4.0",
