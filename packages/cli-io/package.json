{
  "name": "@takomo/cli-io",
  "version": "2.8.0",
  "description": "Takomo CLI IO",
  "author": "henri@takomo.io",
  "license": "MIT",
  "os": [
    "darwin",
    "linux"
  ],
  "engines": {
    "node": ">=14.4.0"
  },
  "homepage": "https://takomo.io",
  "bugs": "https://github.com/takomo-io/takomo/issues",
  "type": "commonjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/takomo-io/takomo.git"
  },
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean:dist": "rm -rf dist",
    "clean:deps": "rm -rf node_modules",
    "clean:all": "rm -rf node_modules dist && rm -f yarn.lock",
    "watch": "tsc -p tsconfig.build.json --watch",
    "depcheck": "depcheck --ignores=@babel/core,@babel/preset-env,@babel/preset-typescript,babel-jest,@types/jest",
    "test": "jest test --passWithNoTests"
  },
  "dependencies": {
    "joi": "17.2.1",
    "@takomo/aws-clients": "2.8.0",
    "@takomo/config-sets": "2.8.0",
    "@takomo/core": "2.8.0",
    "@takomo/deployment-targets": "2.8.0",
    "@takomo/init-command": "2.8.0",
    "@takomo/organization-commands": "2.8.0",
    "@takomo/organization-config": "2.8.0",
    "@takomo/stacks-commands": "2.8.0",
    "@takomo/stacks-context": "2.8.0",
    "@takomo/stacks-model": "2.8.0",
    "@takomo/util": "2.8.0",
    "aws-sdk": "2.745.0",
    "date-and-time": "0.11.1",
    "diff": "4.0.1",
    "easy-table": "1.1.1",
    "inquirer": "7.0.6",
    "inquirer-autocomplete-prompt": "1.0.2",
    "lodash.flatten": "4.4.0",
    "lodash.uniq": "4.5.0",
    "pretty-ms": "5.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.10.2",
    "@babel/preset-env": "7.10.2",
    "@babel/preset-typescript": "7.10.1",
    "@types/date-and-time": "0.6.0",
    "@types/diff": "4.0.2",
    "@types/easy-table": "0.0.32",
    "@types/inquirer": "6.5.0",
    "@types/jest": "26.0.0",
    "@types/lodash.flatten": "4.4.6",
    "@types/lodash.uniq": "4.5.6",
    "babel-jest": "26.0.1",
    "jest": "26.0.1",
    "typescript": "4.0.2"
  }
}
