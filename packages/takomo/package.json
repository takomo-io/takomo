{
  "name": "takomo",
  "version": "1.2.1",
  "description": "Organize, configure and deploy CloudFormation stacks. Manage AWS organization.",
  "author": "henri@takomo.io",
  "license": "MIT",
  "os": [
    "darwin",
    "linux"
  ],
  "engines": {
    "node": ">=10.0.0"
  },
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "aws",
    "cloudformation",
    "cfn",
    "deploy",
    "aws organizations",
    "cli",
    "cloudformation templates",
    "cloudformation stacks",
    "cloudformation deployment"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/takomo-io/takomo.git"
  },
  "homepage": "https://takomo.io",
  "bugs": "https://github.com/takomo-io/takomo/issues",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean:dist": "rm -rf dist",
    "clean:deps": "rm -rf node_modules",
    "clean:all": "rm -rf node_modules dist && rm -f yarn.lock",
    "watch": "tsc -p tsconfig.build.json --watch",
    "depcheck": "depcheck --ignore-dirs=dist --ignores=@types/jest",
    "test": "jest test --passWithNoTests"
  },
  "bin": {
    "tkm": "./bin/tkm",
    "takomo": "./bin/tkm"
  },
  "dependencies": {
    "@takomo/cli": "1.2.1"
  },
  "devDependencies": {
    "typescript": "3.9.3"
  }
}
