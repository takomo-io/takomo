{
  "name": "Takomo",
  "version": "0.0.1",
  "private": true,
  "description": "Takomo monorepo",
  "homepage": "https://takomo.io",
  "bugs": "https://github.com/takomo-io/takomo/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/takomo-io/takomo.git"
  },
  "license": "MIT",
  "author": "henri@takomo.io",
  "workspaces": [
    "packages/*",
    "integration-tests/*"
  ],
  "scripts": {
    "api-docs": "typedoc",
    "bootstrap": "lerna bootstrap",
    "build": "lerna run build",
    "build-if-changed": "lerna run build-if-changed",
    "clean:all": "lerna run 'clean:all' --parallel && rm -rf node_modules && rm -f yarn.lock",
    "clean:deps": "lerna run 'clean:deps' --parallel && rm -rf node_modules",
    "clean:dist": "lerna run 'clean:dist' --parallel",
    "depcheck": "lerna run depcheck --parallel",
    "publish-packages": "lerna publish from-package --yes",
    "publish-version": "lerna version --exact --create-release github",
    "sort-package-json": "sort-package-json 'package.json' 'packages/*/package.json'",
    "test": "lerna run test",
    "watch": "lerna run watch --parallel",
    "prepare": "husky install"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "header-max-length": [
        0,
        "always",
        160
      ],
      "body-max-line-length": [
        0,
        "always",
        160
      ],
      "footer-max-line-length": [
        0,
        "always",
        160
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "7.16.12",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-typescript": "7.16.7",
    "@types/date-and-time": "0.13.0",
    "@types/diff": "4.0.2",
    "@types/folder-hash": "4.0.1",
    "@types/inquirer": "6.5.0",
    "@types/jest": "27.4.0",
    "@types/js-yaml": "4.0.5",
    "@types/json-stable-stringify": "1.0.34",
    "@types/lodash.intersection": "4.4.7",
    "@types/lodash.merge": "4.6.7",
    "@types/lodash.takerightwhile": "4.6.7",
    "@types/lodash.without": "4.4.7",
    "@types/node": "14.17.1",
    "@types/ramda": "0.27.34",
    "@types/semver": "7.3.9",
    "@types/uuid": "8.3.4",
    "@types/yargs": "17.0.2",
    "@types/jmespath": "0.15.0",
    "@typescript-eslint/eslint-plugin": "5.23.0",
    "@typescript-eslint/parser": "5.23.0",
    "babel-jest": "27.4.6",
    "depcheck": "1.4.3",
    "dotenv": "10.0.0",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "27.4.7",
    "jest-mock-extended": "2.0.4",
    "lerna": "5.1.8",
    "nx": "14.4.2",
    "lint-staged": "12.4.1",
    "prettier": "2.7.0",
    "prettier-plugin-organize-imports": "2.3.4",
    "pretty-quick": "3.1.3",
    "sort-package-json": "1.57.0",
    "ts-node": "10.4.0",
    "typedoc": "0.22.17",
    "typescript": "4.7.4",
    "@commitlint/cli": "17.0.2",
    "@commitlint/config-conventional": "17.0.2",
    "husky": "8.0.1"
  },
  "dependencies": {
    "chalk": "4.1.2",
    "date-and-time": "2.1.0",
    "diff": "5.0.0",
    "easy-table": "1.2.0",
    "handlebars": "4.7.7",
    "joi": "17.6.0",
    "js-yaml": "4.1.0",
    "json-stable-stringify": "1.0.1",
    "lodash.merge": "4.6.2",
    "p-queue": "6.6.2",
    "pretty-ms": "7.0.1",
    "strip-ansi": "6.0.0",
    "uuid": "8.3.2",
    "@aws-sdk/client-cloudformation": "3.81.0",
    "@aws-sdk/client-cloudtrail": "3.81.0",
    "@aws-sdk/client-organizations": "3.78.0",
    "@aws-sdk/client-s3": "3.81.0",
    "@aws-sdk/client-secrets-manager": "3.81.0",
    "@aws-sdk/client-ssm": "3.81.0",
    "@aws-sdk/client-sts": "3.81.0",
    "@aws-sdk/credential-provider-node": "3.81.0",
    "@aws-sdk/credential-providers": "3.81.0",
    "@aws-sdk/middleware-retry": "3.80.0",
    "@aws-sdk/node-http-handler": "3.78.0",
    "@aws-sdk/types": "3.78.0",
    "cockatiel": "2.0.1",
    "lodash.takerightwhile": "4.6.0",
    "ramda": "0.28.0",
    "folder-hash": "4.0.2",
    "neverthrow": "4.3.1",
    "jmespath": "0.16.0",
    "dotenv-expand": "5.1.0",
    "readdirp": "3.6.0",
    "semver": "7.3.7"
  }
}
