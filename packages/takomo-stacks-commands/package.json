{
  "name": "@takomo/stacks-commands",
  "version": "1.3.0",
  "description": "Takomo Stacks Commands",
  "author": "henri@takomo.io",
  "license": "MIT",
  "homepage": "https://takomo.io",
  "bugs": "https://github.com/takomo-io/takomo/issues",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/takomo-io/takomo.git"
  },
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean:dist": "rm -rf dist",
    "clean:deps": "rm -rf node_modules",
    "clean:all": "rm -rf node_modules dist && rm -f yarn.lock",
    "watch": "tsc -p tsconfig.build.json --watch",
    "depcheck": "depcheck --ignores=@babel/core,@babel/preset-env,@babel/preset-typescript,babel-jest",
    "test": "jest test --passWithNoTests"
  },
  "dependencies": {
    "@hapi/joi": "16.1.8",
    "@takomo/aws-clients": "1.3.0",
    "@takomo/core": "1.3.0",
    "@takomo/stacks-context": "1.3.0",
    "@takomo/stacks-hooks": "1.3.0",
    "@takomo/stacks-model": "1.3.0",
    "@takomo/stacks-schema": "1.3.0",
    "@takomo/util": "1.3.0",
    "aws-sdk": "2.686.0",
    "uuid": "3.3.3"
  },
  "devDependencies": {
    "jest": "26.0.1",
    "jest-mock-extended": "1.0.8",
    "babel-jest": "26.0.1",
    "@babel/core": "7.10.2",
    "@babel/preset-env": "7.10.2",
    "@babel/preset-typescript": "7.10.1",
    "@types/hapi__joi": "16.0.4",
    "@types/uuid": "3.4.6",
    "typescript": "3.9.3"
  }
}
