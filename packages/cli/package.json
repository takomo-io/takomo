{
  "name": "@takomo/cli",
  "version": "2.4.0",
  "description": "Takomo CLI",
  "author": "henri@takomo.io",
  "license": "MIT",
  "os": [
    "darwin",
    "linux"
  ],
  "engines": {
    "node": ">=14.4.0"
  },
  "homepage": "https://takomo.io",
  "bugs": "https://github.com/takomo-io/takomo/issues",
  "type": "commonjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/takomo-io/takomo.git"
  },
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean:dist": "rm -rf dist",
    "clean:deps": "rm -rf node_modules",
    "clean:all": "rm -rf node_modules dist && rm -f yarn.lock",
    "watch": "tsc -p tsconfig.build.json --watch",
    "depcheck": "depcheck --ignores=@babel/core,@babel/preset-env,@babel/preset-typescript,babel-jest,@types/jest",
    "test": "jest test --passWithNoTests"
  },
  "dependencies": {
    "@takomo/cli-io": "2.4.0",
    "@takomo/config-sets": "2.4.0",
    "@takomo/core": "2.4.0",
    "@takomo/deployment-targets": "2.4.0",
    "@takomo/init-command": "2.4.0",
    "@takomo/organization-commands": "2.4.0",
    "@takomo/stacks-commands": "2.4.0",
    "@takomo/util": "2.3.0",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "easy-table": "1.1.1",
    "lodash.merge": "4.6.2",
    "pretty-ms": "5.1.0",
    "yargs": "13.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.10.2",
    "@babel/preset-env": "7.10.2",
    "@babel/preset-typescript": "7.10.1",
    "@types/easy-table": "0.0.32",
    "@types/jest": "26.0.0",
    "@types/lodash.merge": "4.6.6",
    "@types/yargs": "13.0.3",
    "babel-jest": "26.0.1",
    "jest": "26.0.1",
    "typescript": "3.9.6"
  }
}
