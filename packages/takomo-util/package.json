{
  "name": "@takomo/util",
  "version": "1.1.0",
  "description": "Takomo Util",
  "author": "henri@takomo.io",
  "license": "MIT",
  "homepage": "https://takomo.io",
  "bugs": "https://github.com/takomo-io/takomo/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/takomo-io/takomo.git"
  },
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean:dist": "rm -rf dist",
    "clean:deps": "rm -rf node_modules",
    "clean:all": "rm -rf node_modules dist && rm -f yarn.lock",
    "watch": "tsc -p tsconfig.build.json --watch",
    "depcheck": "depcheck --ignore-dirs=dist",
    "test": "jest test --passWithNoTests"
  },
  "dependencies": {
    "@hapi/joi": "16.1.8",
    "chalk": "3.0.0",
    "date-and-time": "0.11.1",
    "easy-table": "1.1.1",
    "handlebars": "4.7.6",
    "js-yaml": "3.13.1",
    "pretty-ms": "5.1.0"
  },
  "devDependencies": {
    "@types/date-and-time": "0.6.0",
    "@types/easy-table": "0.0.32",
    "@types/hapi__joi": "16.0.4",
    "@types/jest": "25.1.1",
    "@types/js-yaml": "3.12.1",
    "jest": "25.5.3",
    "jest-mock-extended": "1.0.8",
    "ts-jest": "25.2.1",
    "typescript": "3.8.3"
  }
}
