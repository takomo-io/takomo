{
  "name": "@takomo/cli-io",
  "version": "3.2.2",
  "description": "Takomo CLI IO",
  "homepage": "https://takomo.io",
  "bugs": "https://github.com/takomo-io/takomo/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/takomo-io/takomo.git"
  },
  "license": "MIT",
  "author": "henri@takomo.io",
  "type": "commonjs",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build-if-changed": "../../ci/build-if-changed.sh",
    "clean:all": "rm -rf node_modules dist && rm -f yarn.lock && rm -rf .checksum",
    "clean:deps": "rm -rf node_modules",
    "clean:dist": "rm -rf dist",
    "depcheck": "depcheck --config=../../.depcheckrc",
    "test": "jest test --passWithNoTests --verbose",
    "watch": "tsc -p tsconfig.build.json --watch"
  },
  "dependencies": {
    "@takomo/aws-model": "3.1.0",
    "@takomo/config-sets": "3.2.2",
    "@takomo/core": "3.1.0",
    "@takomo/deployment-targets-commands": "3.2.2",
    "@takomo/init-command": "3.1.0",
    "@takomo/organization-commands": "3.2.2",
    "@takomo/organization-config": "3.2.2",
    "@takomo/organization-model": "3.1.0",
    "@takomo/stacks-commands": "3.2.2",
    "@takomo/stacks-model": "3.2.1",
    "@takomo/util": "3.1.0",
    "date-and-time": "0.14.2",
    "diff": "5.0.0",
    "easy-table": "1.1.1",
    "inquirer": "7.0.6",
    "inquirer-autocomplete-prompt": "1.0.2",
    "joi": "17.2.1",
    "lodash.flatten": "4.4.0",
    "lodash.uniq": "4.5.0",
    "pretty-ms": "5.1.0",
    "ramda": "0.27.1",
    "ts-dedent": "2.0.0"
  },
  "devDependencies": {
    "@takomo/test-unit": "3.1.0"
  },
  "engines": {
    "node": ">=14.4.0"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "publishConfig": {
    "access": "public"
  }
}
