{
  "name": "@takomo/stacks-hooks",
  "version": "2.4.0",
  "description": "Takomo Stacks Hooks",
  "author": "henri@takomo.io",
  "license": "MIT",
  "os": [
    "darwin",
    "linux"
  ],
  "engines": {
    "node": ">=14.4.0"
  },
  "homepage": "https://takomo.io",
  "bugs": "https://github.com/takomo-io/takomo/issues",
  "type": "commonjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/takomo-io/takomo.git"
  },
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean:dist": "rm -rf dist",
    "clean:deps": "rm -rf node_modules",
    "clean:all": "rm -rf node_modules dist && rm -f yarn.lock",
    "watch": "tsc -p tsconfig.build.json --watch",
    "depcheck": "depcheck --ignores=@babel/core,@babel/preset-env,@babel/preset-typescript,babel-jest,@types/jest",
    "test": "jest test --passWithNoTests"
  },
  "dependencies": {
    "@hapi/joi": "17.1.1",
    "@takomo/core": "2.4.0",
    "@takomo/stacks-model": "2.4.0",
    "@takomo/util": "2.3.0",
    "aws-sdk": "2.713.0"
  },
  "devDependencies": {
    "@babel/core": "7.10.2",
    "@babel/preset-env": "7.10.2",
    "@babel/preset-typescript": "7.10.1",
    "@types/hapi__joi": "17.1.2",
    "@types/jest": "26.0.0",
    "babel-jest": "26.0.1",
    "jest": "26.0.1",
    "jest-mock-extended": "1.0.8",
    "typescript": "3.9.5"
  }
}
