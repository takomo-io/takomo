    "@types/lodash.merge": "4.6.6",
    "@types/lodash.takerightwhile": "4.6.6",
    "@types/lodash.uniq": "4.5.6",
    "@types/lodash.uniqby": "4.7.6",
    "@types/lodash.without": "4.4.6",
    "jest": "25.5.3",
    "jest-mock-extended": "1.0.8",
    "ts-jest": "25.2.1",
    "typescript": "3.8.3"
  }
