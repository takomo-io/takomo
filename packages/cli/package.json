{
  "name": "@takomo/cli",
  "version": "3.0.0-alpha.1",
  "description": "Takomo CLI",
  "homepage": "https://takomo.io",
  "bugs": "https://github.com/takomo-io/takomo/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/takomo-io/takomo.git"
  },
  "license": "MIT",
  "author": "henri@takomo.io",
  "type": "commonjs",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "postbuild": "cp package.json dist/",
    "build-if-changed": "../../ci/build-if-changed.sh",
    "clean:all": "rm -rf node_modules dist && rm -f yarn.lock",
    "clean:deps": "rm -rf node_modules",
    "clean:dist": "rm -rf dist",
    "depcheck": "depcheck --config=../../.depcheckrc",
    "test": "jest test --passWithNoTests --verbose",
    "watch": "tsc -p tsconfig.build.json --watch"
  },
  "dependencies": {
    "@takomo/aws-model": "^3.0.0-alpha.1",
    "@takomo/cli-io": "^3.0.0-alpha.1",
    "@takomo/config-repository-fs": "^3.0.0-alpha.1",
    "@takomo/config-sets": "^3.0.0-alpha.1",
    "@takomo/core": "^3.0.0-alpha.1",
    "@takomo/deployment-targets-commands": "^3.0.0-alpha.1",
    "@takomo/init-command": "^3.0.0-alpha.1",
    "@takomo/organization-commands": "^3.0.0-alpha.1",
    "@takomo/organization-model": "^3.0.0-alpha.0",
    "@takomo/stacks-commands": "^3.0.0-alpha.1",
    "@takomo/stacks-model": "^3.0.0-alpha.1",
    "@takomo/util": "^3.0.0-alpha.1",
    "aws-sdk": "2.745.0",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "easy-table": "1.1.1",
    "joi": "17.2.1",
    "lodash.merge": "4.6.2",
    "pretty-ms": "5.1.0",
    "semver": "7.3.4",
    "yargs": "13.3.0"
  },
  "engines": {
    "node": ">=14.4.0"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "publishConfig": {
    "access": "public"
  }
}
