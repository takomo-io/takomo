{
  "name": "@takomo/cli-io",
  "version": "1.4.1",
  "description": "Takomo CLI IO",
  "author": "henri@takomo.io",
  "license": "MIT",
  "homepage": "https://takomo.io",
  "bugs": "https://github.com/takomo-io/takomo/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/takomo-io/takomo.git"
  },
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean:dist": "rm -rf dist",
    "clean:deps": "rm -rf node_modules",
    "clean:all": "rm -rf node_modules dist && rm -f yarn.lock",
    "watch": "tsc -p tsconfig.build.json --watch",
    "depcheck": "depcheck --ignore-dirs=dist --ignores=@types/jest",
    "test": "jest test --passWithNoTests"
  },
  "dependencies": {
    "@hapi/joi": "16.1.8",
    "@takomo/aws-clients": "1.4.1",
    "@takomo/config-sets": "1.4.1",
    "@takomo/core": "1.3.0",
    "@takomo/deployment-targets": "1.4.1",
    "@takomo/init-command": "1.3.0",
    "@takomo/organization": "1.4.1",
    "@takomo/stacks-commands": "1.4.1",
    "@takomo/stacks-context": "1.4.1",
    "@takomo/stacks-model": "1.3.0",
    "@takomo/util": "1.3.0",
    "aws-sdk": "2.686.0",
    "date-and-time": "0.11.1",
    "diff": "4.0.1",
    "easy-table": "1.1.1",
    "inquirer": "7.0.6",
    "inquirer-autocomplete-prompt": "1.0.2",
    "pretty-ms": "5.1.0"
  },
  "devDependencies": {
    "@types/date-and-time": "0.6.0",
    "@types/diff": "4.0.2",
    "@types/easy-table": "0.0.32",
    "@types/hapi__joi": "16.0.4",
    "@types/inquirer": "6.5.0",
    "@types/jest": "25.2.3",
    "jest": "26.0.1",
    "ts-jest": "26.0.0",
    "typescript": "3.9.3"
  }
}
