{
  "name": "@takomo/deployment-targets-repository",
  "version": "4.2.0",
  "description": "Takomo Deployment Targets Repository",
  "homepage": "https://takomo.io",
  "bugs": "https://github.com/takomo-io/takomo/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/takomo-io/takomo.git"
  },
  "license": "MIT",
  "author": "henri@takomo.io",
  "type": "commonjs",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build-if-changed": "../../ci/build-if-changed.sh",
    "clean:all": "rm -rf node_modules dist && rm -f yarn.lock && rm -rf .checksum",
    "clean:deps": "rm -rf node_modules",
    "clean:dist": "rm -rf dist",
    "depcheck": "depcheck --config=../../.depcheckrc",
    "test": "jest test --passWithNoTests --verbose",
    "watch": "tsc -p tsconfig.build.json --watch"
  },
  "dependencies": {
    "@takomo/aws-model": "4.2.0",
    "@takomo/core": "4.2.0",
    "@takomo/deployment-targets-config": "4.2.0",
    "@takomo/deployment-targets-model": "4.2.0",
    "@takomo/deployment-targets-schema": "4.2.0",
    "@takomo/util": "4.2.0",
    "joi": "17.6.0",
    "readdirp": "3.6.0"
  },
  "engines": {
    "node": ">=14.17.1"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "publishConfig": {
    "access": "public"
  }
}
