{
  "name": "@takomo/unit-test",
  "version": "2.7.0",
  "description": "Takomo unit test utils",
  "author": "henri@takomo.io",
  "license": "MIT",
  "os": [
    "darwin",
    "linux"
  ],
  "engines": {
    "node": ">=14.4.0"
  },
  "homepage": "https://takomo.io",
  "bugs": "https://github.com/takomo-io/takomo/issues",
  "type": "commonjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/takomo-io/takomo.git"
  },
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean:dist": "rm -rf dist",
    "clean:deps": "rm -rf node_modules",
    "clean:all": "rm -rf node_modules dist && rm -f yarn.lock",
    "watch": "tsc -p tsconfig.build.json --watch",
    "depcheck": "depcheck --ignores=@babel/core,@babel/preset-env,@babel/preset-typescript,babel-jest,@types/jest",
    "test": "jest test --passWithNoTests"
  },
  "dependencies": {
    "joi": "17.2.1"
  },
  "devDependencies": {
    "@types/jest": "26.0.0",
    "babel-jest": "26.0.1",
    "dotenv": "8.2.0",
    "jest": "26.0.1",
    "typescript": "4.0.2"
  }
}
