{
  "name": "takomo",
  "version": "5.0.0-alpha.0",
  "description": "Organize, configure and deploy CloudFormation stacks.",
  "keywords": [
    "aws",
    "cloudformation",
    "cfn",
    "deploy",
    "cli",
    "cloudformation templates",
    "cloudformation stacks",
    "cloudformation deployment"
  ],
  "homepage": "https://takomo.io",
  "bugs": "https://github.com/takomo-io/takomo/issues",
  "repository": {
    "type": "git",
    "url": "git@github.com:takomo-io/takomo.git"
  },
  "license": "MIT",
  "author": "henri@takomo.io",
  "type": "commonjs",
  "main": "dist/index",
  "types": "dist/index",
  "bin": {
    "takomo": "bin/tkm",
    "tkm": "bin/tkm"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16.17.0"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepare": "husky install",
    "build": "tsc",
    "test": "jest --selectProjects test --maxWorkers=10 --ci --verbose",
    "integration-test": "jest --selectProjects integration-test --maxWorkers=10 --ci --verbose",
    "clean": "rm -f package-lock.json && rm -rf node_modules && rm -rf dist"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "header-max-length": [
        0,
        "always",
        160
      ],
      "body-max-line-length": [
        0,
        "always",
        160
      ],
      "footer-max-line-length": [
        0,
        "always",
        160
      ]
    }
  },
  "devDependencies": {
    "@aws-sdk/client-ec2": "3.186.0",
    "@babel/core": "7.16.12",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-typescript": "7.16.7",
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "@jest/console": "29.1.2",
    "@takomo/test-custom-helper-lowercase": "0.0.1",
    "@takomo/test-custom-helper-uppercase": "0.0.1",
    "@takomo/test-custom-resolver-code": "0.0.1",
    "@takomo/test-custom-resolver-name": "0.0.1",
    "@types/date-and-time": "0.13.0",
    "@types/diff": "4.0.2",
    "@types/folder-hash": "4.0.1",
    "@types/inquirer": "6.5.0",
    "@types/jest": "27.4.0",
    "@types/jmespath": "0.15.0",
    "@types/js-yaml": "4.0.5",
    "@types/json-stable-stringify": "1.0.34",
    "@types/lodash.merge": "4.6.7",
    "@types/lodash.takerightwhile": "4.6.7",
    "@types/node": "14.17.1",
    "@types/ramda": "0.27.34",
    "@types/semver": "7.3.9",
    "@types/tmp": "0.2.3",
    "@types/uuid": "8.3.4",
    "@types/yargs": "17.0.2",
    "@typescript-eslint/eslint-plugin": "5.23.0",
    "@typescript-eslint/parser": "5.23.0",
    "babel-jest": "27.4.6",
    "depcheck": "1.4.3",
    "eslint": "8.22.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.1",
    "jest": "27.4.7",
    "jest-environment-testenv-recycler": "0.0.19",
    "jest-mock-extended": "2.0.4",
    "prettier": "2.7.1",
    "prettier-plugin-organize-imports": "3.1.1",
    "pretty-quick": "3.1.3",
    "tmp": "0.2.1",
    "ts-node": "10.9.1",
    "typedoc": "0.23.17",
    "typescript": "4.7.4",
    "semantic-release": "19.0.5",
    "npm-check-updates": "16.3.14"
  },
  "dependencies": {
    "@aws-sdk/client-cloudformation": "3.186.0",
    "@aws-sdk/client-cloudtrail": "3.187.0",
